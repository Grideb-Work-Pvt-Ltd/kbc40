                                           {
                                               i++;
                                            <tr>
                                                <td>@i</td>
                                                <td>@item.RankName</td>
                                                <td>
                                                    <form asp-action="otp" asp-controller="user" method="post">
                                                        <div class="form-group" @item.c2>                  
                                                            <button type="submit" for="exampleFormControlSelect1" style="color: white; background-color: #ffc107;">Send OTP:</button>
                                                        </div>
                                                    </form>
                                                </td>
                                                <td>
                                                    <span @item.c1>Updated</span>
                                                    <form asp-action="tree" asp-controller="user" method="post">
                                                        <div class="form-group" @item.c2>
                                                            <input type="text" name="topenter" value="" required>
                                                            <input type="text" name="rank" value="@item.RankName" hidden>
                                                            <button type="submit" for="exampleFormControlSelect1" style="color: white; background-color: #ffc107;">Upgrade:</button>
                                                        </div>
                                                    </form>
                                                    <span @item.c3>NotAligible</span>
                                                </td>
                                            </tr>
                                            }
